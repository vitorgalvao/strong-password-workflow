				<key>script</key>
				<string>function make_item {
  local -r pass_length="${1}"
  local -r special_set='A-Za-z0-9!@#$%^&amp;*()_+-=[]{}|;:,.&lt;&gt;?~'
  local -r alpha_set='A-Za-z0-9'

  local -r special_pass="$(LC_ALL=C tr -dc "${special_set}" &lt; /dev/urandom |
    head --bytes "${pass_length}")"

  local -r alpha_pass="$(LC_ALL=C tr -dc "${alpha_set}" &lt; /dev/urandom |
    head --bytes "${pass_length}")"

  printf '{
    "uid": "%s",
    "title": "%s Characters",
    "subtitle": "%s",
    "arg": "%s",
    "mods": { "cmd": { "subtitle": "%s", "arg": "%s" } }
  }' \
  "${pass_length}" "${pass_length}"   \
  "${special_pass}" "${special_pass}" \
  "${alpha_pass}" "${alpha_pass}"
}

printf '{ "items": [ %s, %s, %s, %s ] }' \
  "$(make_item 128)" \
  "$(make_item 64)"  \
  "$(make_item 32)"  \
  "$(make_item 16)"</string>
