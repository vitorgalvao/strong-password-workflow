				<string>function run(argv) {
  ObjC.import("AppKit")

  const pboard = $.NSPasteboard.generalPasteboard

  pboard.clearContents
  pboard.setStringForType(argv[0], "org.nspasteboard.ConcealedType")
  pboard.setStringForType(argv[0], "public.utf8-plain-text")
}</string>
