				<key>script</key>
				<string>function make_item {
  local -r word_count="${1}"

  local -r pass="$(grep --extended-regexp '^.{4,12}$' &lt; '/usr/share/dict/words' |
    sort --random-sort           |
    head --lines "${word_count}" |
    tr '\n' '-'                  |
    sed 's/-$//')"

  printf '{
    "uid": "%s",
    "title": "%s Words",
    "subtitle": "%s",
    "arg": "%s"
  }' \
  "${word_count}" "${word_count}" \
  "${pass}" "${pass}"
}

printf '{ "items": [ %s, %s, %s, %s ] }' \
  "$(make_item 10)" \
  "$(make_item 8)"  \
  "$(make_item 6)"  \
  "$(make_item 4)"</string>
